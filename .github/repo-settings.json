{
  "branchProtection": {
    "main": {
      "required_status_checks": {
        "strict": true,
        "contexts": [
          "CodeQL",
          "Analyze (javascript)",
          "assignAuthor",
          "deploy",
          "prchecks",
          "e2e-tests"
        ]
      },
      "enforce_admins": true,
      "required_pull_request_reviews": {
        "dismiss_stale_reviews": true,
